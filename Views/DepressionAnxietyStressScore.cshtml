@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.DepressionAnxietyStressScore>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
    Layout = "master.cshtml";
}

<style>
    .assessment-form {
        margin: 40px 0;
    }

    .note-2 {
        font-size: 18px;
        margin-bottom: 10px;
    }

    .thermometer-img {
        width: 70%;
        margin: 0 0 20px 0;
    }
</style>

<input type="hidden" id="DASSNodeId" value="@Model.Id" />

<!-- banner section -->
<section class="patient-form-banner common-banner">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="carousel-caption">
                    <h1 class="banner-heading text-white">Depression, Anxiety, Stress etc Score DASS21</h1>
                    <p>Text: NCC welcomes HCPs to partner with us. You will have access to our latest material, ongoing events and the privilege to directly refer your patients to us.</p>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="assessment-form">
    <div class="container">
        <div class="row">
            <div class="col-12 text-center">
                <h2 class="common-heading">DASS21</h2>
            </div>
